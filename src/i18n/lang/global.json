{
  "validate": {
    "willThrow": "function:testThrow",
    "useFallbackValidator": "function:onlyFallback",
    "badRegex": {
      "pattern": "[",
      "flags": ""
    },
    "slashPattern": "/^abc$/i",
    "isHexadecimal": {
      "pattern": "^[0-9A-F]+$",
      "flags": "i"
    },
    "isBIC": {
      "pattern": "^[A-Z]{4}[A-Z]{2}[A-Z0-9]{2}(?:[A-Z0-9]{3})?$",
      "flags": "i"
    },
    "isUUID": {
      "ALL": {
        "pattern": "^[0-9A-F]{8}-[0-9A-F]{4}-[1-5][0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$",
        "flags": "i"
      },
      "3": {
        "pattern": "^[0-9A-F]{8}-[0-9A-F]{4}-3[0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$",
        "flags": "i"
      },
      "4": {
        "pattern": "^[0-9A-F]{8}-[0-9A-F]{4}-4[0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$",
        "flags": "i"
      },
      "5": {
        "pattern": "^[0-9A-F]{8}-[0-9A-F]{4}-5[0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$",
        "flags": "i"
      }
    },
    "isISINCode": {
      "pattern": "^[A-Z]{2}[A-Z0-9]{9}\\d{1}$",
      "flags": "i"
    },
    "isISSN": {
      "pattern": "^\\d{7}[\\dX]$",
      "flags": "i"
    },
    "isISRC": {
      "pattern": "^[A-Z]{2}[A-Z0-9]{3}\\d{7}$",
      "flags": "i"
    },
    "isISBN10": {
      "pattern": "^(\\d{9})[\\dX]$",
      "flags": "i"
    },
    "isISBN13": {
      "pattern": "^(978|979)\\d{10}$",
      "flags": "i"
    },
    "isHexColor": {
      "pattern": "^#([0-9A-F]{3}|[0-9A-F]{4}|[0-9A-F]{6}|[0-9A-F]{8})$",
      "flags": "i"
    },
    "isIMEI": {
      "pattern": "^\\d{15}$",
      "flags": ""
    },
    "ISO3166-1-alpha-2": {
      "pattern": "^[A-Z]{2}$"
    },
    "ISO3166-1-alpha-3": {
      "pattern": "^[A-Z]{3}$"
    },
    "ISO639-1": {
      "pattern": "^[a-z]{2}$"
    },
    "ISO4217": {
      "pattern": "^[A-Z]{3}$"
    },
    "isMongoId": {
      "pattern": "^[0-9A-F]{24}$",
      "flags": "i"
    },
    "isDataURI": {
      "pattern": "^data:([^,]+)?(?:;base64)?,(.+)$",
      "flags": "i"
    },
    "isHSL": {
      "pattern": "^(?:hsla\\(\\s*(0|[1-9]\\d?|[12]\\d{2}|3[0-5]\\d|360)\\s*,\\s*(0|100|[1-9]\\d?)%\\s*,\\s*(0|100|[1-9]\\d?)%\\s*,\\s*(0|1|0?\\.\\d+)\\s*\\)|hsl\\(\\s*(0|[1-9]\\d?|[12]\\d{2}|3[0-5]\\d|360)\\s*,\\s*(0|100|[1-9]\\d?)%\\s*,\\s*(0|100|[1-9]\\d?)%(?:\\s*\\/\\s*(0|1|0?\\.\\d+|[1-9]\\d?%|100%))?\\s*\\))$",
      "flags": "i"
    },
    "isMimeType": {
      "pattern": "^[a-zA-Z0-9-]+\\/[a-zA-Z0-9.+-]+$",
      "flags": "i"
    },
    "isMultibyte": {
      "pattern": "[^\\x00-\\x7F]"
    },
    "isAscii": {
      "pattern": "^[\\x00-\\x7F]+$"
    },
    "isSemVer": {
      "pattern": "^v?(0|[1-9]\\d*)\\.(0|[1-9]\\d*)\\.(0|[1-9]\\d*)(?:-((?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\\.(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\\+([0-9a-zA-Z-]+(?:\\.[0-9a-zA-Z-]+)*))?$",
      "flags": ""
    },
    "creditCardPatterns": {
      "amex": {
        "pattern": "^3[47][0-9]{13}$"
      },
      "dinersclub": {
        "pattern": "^3(?:0[0-5]|[68][0-9])[0-9]{11}$"
      },
      "discover": {
        "pattern": "^6(?:011|5[0-9][0-9])[0-9]{12,15}$"
      },
      "jcb": {
        "pattern": "^(?:2131|1800|35\\d{3})\\d{11}$"
      },
      "mastercard": {
        "pattern": "^((?:5[1-5][0-9]{2}|(222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720))[0-9]{12}|(?:5[0678]\\d\\d|6304|6390|67\\d\\d)\\d{8,15})$"
      },
      "unionpay": {
        "pattern": "^(6[27][0-9]{14}|^(81[0-9]{14,17}))$"
      },
      "visa": {
        "pattern": "^4[0-9]{12}(?:[0-9]{3,6})?$"
      },
      "maestro": {
        "pattern": "^(5018|5020|5031|5038|56|58|6304|6759|6761|6763)\\d{8,15}$"
      }
    },
    "isSurrogatePair": {
      "pattern": "[\\uD800-\\uDBFF][\\uDC00-\\uDFFF]"
    },
    "isMACAddress": {
      "COLON": {
        "pattern": "^([0-9A-F]{2}:){5}[0-9A-F]{2}$",
        "flags": "i"
      },
      "HYPHEN": {
        "pattern": "^([0-9A-F]{2}-){5}[0-9A-F]{2}$",
        "flags": "i"
      },
      "DOT": {
        "pattern": "^([0-9A-F]{4}\\.){2}[0-9A-F]{4}$",
        "flags": "i"
      }
    }
  },
  "messages": {
    "invalidType": "The provided value is not a string.",
    "isEmpty": "The field cannot be empty.",
    "validateByteLengthMin": "The field must be at least {min} bytes.",
    "validateByteLengthMax": "The field must be at most {max} bytes."
  }
}
