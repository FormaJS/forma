{
  "name": "English (India)",
  "separators": { "decimal": ".", "thousand": ",", "thousandDigits": 3 },
  "currency": {
    "symbol": "â‚¹",
    "code": "INR",
    "symbol_before_digits": true,
    "space_between_symbol_and_digits": false,
    "decimal_digits": 2,
    "negative_format": ["-{symbol}{amount}", "({symbol}{amount})", "{symbol}({amount})"]
  },
  "date": { "format": "dd/mm/yyyy", "separator": "/" },
  "masks": {
    "postalcode": { "default": "######" },
    "mobileNumber": { "local": "#####-#####", "international": "+91 #####-#####" },
    "taxId": { "PAN": "AAAAA9999A" }
  },
  "validate": {
    "alpha": { "pattern": "^[A-Z]+$", "flags": "i" },
    "alphanumeric": { "pattern": "^[A-Z0-9]+$", "flags": "i" },
    "mobilenumber": { "pattern": "^(?:\\+?91[\\s-]?)?[6-9]\\d{9}$" },
    "postalcode": { "pattern": "^\\d{6}$" },
    "licenseplate": {
      "pattern": "^[A-Z]{2}[\\s-]?\\d{1,2}[\\s-]?[A-Z]{1,2}[\\s-]?\\d{4}$",
      "flags": "i"
    },
    "boolean": {
      "strictTrue": ["true"],
      "strictFalse": ["false"],
      "looseTrue": ["1", "yes", "y"],
      "looseFalse": ["0", "no", "n"]
    },
    "taxid": "function:validateTaxId"
  },
  "messages": {}
}
